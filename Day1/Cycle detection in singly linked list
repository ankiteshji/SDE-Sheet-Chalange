'''
Following is the structure of the Node class already defined.

class Node :
    def __init__(self, data) :
        self.data = data
        self.next = None
'''

def detectCycle(head) :
    
    vis=set()

    while head:
        if head.next in vis:
            return True
        else:
            vis.add(head.next)
        head=head.next
    return False
