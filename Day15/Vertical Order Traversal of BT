
from collections import deque,defaultdict


def verticalOrderTraversal(root):
	# Write your code here
	# Return a list
    hmap = defaultdict(lambda: defaultdict(list))
    queue = deque([(root, 0, 0)])
    while queue:
        temp, x, y = queue.popleft()
        hmap[x][y].append(temp.data)
        
        if temp.left:
            queue.append((temp.left, x - 1, y + 1))
            
        if temp.right:
            queue.append((temp.right, x + 1, y + 1))
    ans = []
    for _, verticals in sorted(hmap.items()):
        vertical = []
        for _, levels in sorted(verticals.items()):
            vertical.extend(levels)
        if vertical:
            ans.extend((vertical))
    for i,item in enumerate(ans):
        ans[i]=int(item)
    return ans
