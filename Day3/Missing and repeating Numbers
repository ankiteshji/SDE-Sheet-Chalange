from math import *
from collections import *
from sys import *
from os import *

def missingAndRepeating(arr, n):
    # Write your code here
    hmap={}
    missing=-1
    repeating=-1
    
    for i,item in enumerate(arr):
        if item in hmap:
            repeating=item
        else:
            hmap[item]=1
    count=1        
    # for key in hmap.keys():
    while count in hmap:
        count+=1
    missing=count

    return [missing,repeating]
