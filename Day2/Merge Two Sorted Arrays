from math import *
from collections import *
from sys import *
from os import *

def ninjaAndSortedArrays(arr1,arr2,m,n):
    # Write your code here.
    
    last=m+n-1

    while(m>0 and n>0):
        if arr1[m-1]<=arr2[n-1]:
            arr1[last]=arr2[n-1]
            n-=1
        else:
            arr1[last]=arr1[m-1] 
            m-=1
        last-=1

    while(n>0):
        arr1[last]=arr2[n-1]
        n,last=n-1,last-1
    return arr1

